group		name_11 node atom block x inf inf 1. 0. 0. y inf 20. 0. 1. 0. z inf 14.39 0. 0. 1. t 3 20. 12. 4. 4. 1. t t t vel 4. 0. 0. time 1500 2000 disp 0. 0.7071 f

# group 12
group		name_12 node atom block x inf inf 1. 0. 0. y 20. 30. 0. 1. 0. z inf 14.39 0. 0. 1. t 3 20. 12. 4. 4. 1. t t t vel 4. 0. 0. time 1500 2000 disp 0. 0.7071 t t 2 1 20. 30.

# group 13
group		name_13 node atom block x inf inf 1. 0. 0. y 30. inf 0. 1. 0. z inf 14.39 0. 0. 1. t 3 20. 12. 4. 4. 1. t t t vel 0. 0. 0. time 1500 2000 disp 0. 100. f

# group 14
group		name_14 node atom block x inf inf 1. 0. 0. y inf 10. 0. 1. 0. z inf 14.39 0. 0. 1. t 3 20. 12. 4. 4. 1. t t t vel 4. 0. 0. time 2000 2500 disp 0. 0.7071 f

# group 15
group		name_15 node atom block x inf inf 1. 0. 0. y 10. 20. 0. 1. 0. z inf 14.39 0. 0. 1. t 3 20. 12. 4. 4. 1. t t t vel 4. 0. 0. time 2000 2500 disp 0. 0.7071 t t 2 1 10. 20.

# group 16
group		name_16 node atom block x inf inf 1. 0. 0. y 20. inf 0. 1. 0. z inf 14.39 0. 0. 1. t 3 20. 12. 4. 4. 1. t t t vel 0. 0. 0. time 2000 2500 disp 0. 100. f

# number of calculation
cal_num		0

# calculate 1
cal		name_1 energy

# calculate 2
cal		name_1 stress

# calculate 3
cal		name_3 stress
